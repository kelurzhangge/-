.cue{
  background: #dc6142;
  position: fixed;
  font-size: 30rpx;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  transform: scale(0);
  -webkit-transform: scale(0);
  opacity: 0;
  transition: all .5s;
  -webkit-transition: all .5s;
}
.cue.on{
  transform: scale(1);
  -webkit-transform: scale(1);
  opacity: 1;
}
.cue .cue_top{
  width: 368rpx;
  height: 86rpx;
  margin: 70rpx auto;
  position: relative;
}
.cue .cue_top image{
  width: 100%;
  height: 100%;
}
.cue .cue_top view{
  width: 220rpx;
  line-height: 86rpx;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  top: 0;
  right: 10rpx;
}

.cue .cue_con{
  width: 600rpx;
  height: 600rpx;
  margin: 0 auto;
  background: #222;
  position: relative;
}
.cue .cue_con .img{
  overflow: hidden;
  position: absolute;
  z-index: 10;
  border: 1px solid #fff;
  transition: top .3s,left .3s;
  -webkit-transition: top .3s,left .3s;
}
.cue .cue_con .img.in{
  z-index: 15;
}
.cue .cue_con .img.on{
  z-index: 20;
  border-color: #ff0000;
}
.cue .cue_con .img image{
  position: absolute;
}
.cue .cue_con .timeout{
  width: 260rpx;
  height: 260rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top:-130rpx;
  margin-left: -130rpx; 
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 999;
  transform: scale(1);
  -webkit-transform: scale(1);
  opacity: 1;
  transition: all .3s;
  -webkit-transition: all .3s;
}
.cue .cue_con .timeout.on{
  transform: scale(0);
  -webkit-transform: scale(0);
  opacity: 0;
}

.cue .cue_con .timeout view{
  width: 100%;
  line-height: 260rpx;
  text-align: center;
  font-size: 180rpx;
  color: #fff;
  font-weight: 700;
}

.cue .cue_point{
  margin-top: 60rpx;
  text-align: center;
}
.cue .cue_bottom{
  position: fixed;
  top: 924rpx;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  justify-content:center;
  align-items:center;
}
.cue .cue_btn{
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  position: relative;
}
.cue .cue_btn image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}


.end{
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  font-size: 30rpx;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  transform: scale(0);
  -webkit-transform: scale(0);
  opacity: 0; 
  transition: all .5s;
  -webkit-transition: all .5s;
}
.end.on{
  transform: scale(1);
  -webkit-transform: scale(1);
  opacity: 1;
}
.end .end_bao{
  width: 600rpx;
  height: 720rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -300rpx;
  margin-top: -360rpx;
}
.end .end_bao .end_bg{
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
}
.end .end_bao .end_bg.end_bg3{
  width: 100%;
  height: 831rpx;
}
.end .end_bao .end_bg.out{
  opacity: 0;
}
.end .end_bao .end_bg.in{
  opacity: 1;
}
.end .end_bao .end_tou{
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  top: 62rpx;
  left: 240rpx;
} 
.end .end_bao .end_tou image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.end .end_bao .end_text{
  width: 100%;
  line-height: 60rpx;
  position: absolute;
  top:210rpx;
  text-align: center;
}
.end .end_bao .end_btn{
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  position: absolute;
  left: 210rpx;
  top: 324rpx;
  
}
.end .end_bao .end_btn.run{
  animation: circle 1s linear infinite;
	-webkit-animation: circle 1s linear infinite;
}
@keyframes circle{
	0%{transform: rotateY(0deg)}
  10%{transform: rotateY(-10deg)}
  50%{transform: rotateY(-180deg)}
  90%{transform: rotateY(-350deg)}
	100%{transform: rotateY(-360deg)}
}
 @-webkit-keyframes circle{
	0%{transform: rotateY(0deg)}
  10%{transform: rotateY(-10deg)}
  50%{transform: rotateY(-180deg)}
  90%{transform: rotateY(-350deg)}
	100%{transform: rotateY(-360deg)}
} 
.end .end_bao .end_btn image{
  width: 100%;
  height: 100%;
}
